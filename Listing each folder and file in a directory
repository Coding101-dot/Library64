import os

for foldername, subfolders, filenames, in os.walk('C://Users//thomas//Desktop//File1'):
    print("The current folder is = " + foldername)

    for subfolder in subfolders:
        print('Subfolder of = ' + foldername + ':' + subfolder)

        for filename in filenames:
            print('The files inside = ' + foldername + 'is :' + filename)

    print('')
